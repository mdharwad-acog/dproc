{
  "name": "@aganitha/dproc",
  "version": "1.0.2",
  "description": "AI-powered data processing engine with multi-format connectors, LLM integration, natural language search, and intelligent reporting",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "data-processing",
    "etl",
    "ai",
    "llm",
    "gemini",
    "openai",
    "deepseek",
    "csv",
    "json",
    "xml",
    "parquet",
    "reports",
    "search",
    "natural-language",
    "pdf-export",
    "html-export",
    "mdx"
  ],
  "author": "acog-mdharwad",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdharwad-acog/dproc.git"
  },
  "publishConfig": {
    "registry": "https://npm.aganitha.ai/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@ai-sdk/google": "^2.0.43",
    "@ai-sdk/openai": "^2.0.72",
    "ai": "^5.0.102",
    "csv-parse": "^6.1.0",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^5.3.2",
    "gray-matter": "^4.0.3",
    "keytar": "^7.9.0",
    "marked": "^17.0.1",
    "node-machine-id": "^1.1.12",
    "nunjucks": "^3.2.4",
    "parquetjs": "^0.11.2",
    "puppeteer": "^24.31.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.10.1",
    "@types/nunjucks": "^3.2.6",
    "@types/parquetjs": "^0.10.6",
    "@vitest/coverage-v8": "^4.0.14",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}